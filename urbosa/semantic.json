{
  "base": "semantic",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/fui/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/raw_components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": 755,
  "autoInstall": true,
  "rtl": false,
  "components": ["visibility", "dimmer", "transition"],
  "version": "2.8.6"
}